{
  "files.autoSave": "onWindowChange",
  "editor.fontSize": 9,
  "editor.fontFamily": "Jetbrains Mono, monospace",
  "editor.tabSize": 2,
  "workbench.colorTheme": "Copilot Theme",
  "editor.formatOnPaste": true,
